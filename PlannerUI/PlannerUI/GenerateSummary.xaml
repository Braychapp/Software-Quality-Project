<Window x:Class="PlannerUI.GenerateSummary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlannerUI"
        mc:Ignorable="d"
        Title="GenerateSummary" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="108,61,0,0" TextWrapping="Wrap" Text="Summary" VerticalAlignment="Top"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="108,82,63,81" SelectionChanged="DataGrid_SelectionChanged"/>
        <Button Content="Navigate to Planner Menu" Margin="108,10,513,378" Click="Button_Click"/>

    </Grid>
</Window>
