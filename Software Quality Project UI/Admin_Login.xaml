<Window x:Class="Software_Quality_Project_UI.Admin_Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Software_Quality_Project_UI"
        mc:Ignorable="d"
        Title="Admin_Login" ResizeMode="NoResize" Height="450" Width="800">
    <Grid>
        <Label Content="Admin Login" HorizontalAlignment="Center" Margin="0,49,0,0" VerticalAlignment="Top" FontSize="22"/>
        <TextBox x:Name="userName"  HorizontalAlignment="Left" Margin="234,156,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="341" Height="37" FontSize="22"/>
        <PasswordBox x:Name="Password"  HorizontalAlignment="Left" Margin="234,217,0,0" VerticalAlignment="Top" Width="341" Height="37" FontSize="22"/>
        <Button Content="Login" HorizontalAlignment="Center" Margin="0,301,0,0" VerticalAlignment="Top" FontSize="22" Height="40" Width="104" Click="Login_Click"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="58,49,0,0" VerticalAlignment="Top" FontSize="22" Height="40" Width="104" Click="Button_Click"/>

    </Grid>
</Window>
